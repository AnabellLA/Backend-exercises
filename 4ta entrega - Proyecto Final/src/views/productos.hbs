    <div class="jumbotron">
    <h1>Lista de productos</h1>
    <br>
    {{#if hayProductos}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
                <th>Descripción</th>
                <th>Código</th>
                <th>Stock</th>
            </tr>
            {{#each productos}}
            <tr>
                <td>{{this.nombre}}</td>
                <td>S/ {{this.precio}}</td>
                <td><img width="50" src={{this.url}} alt="not found"></td>
                <td>{{this.descripcion}}</td>
                <td>{{this.codigo}}</td>
                <td>{{this.stock}}</td>
            </tr>
            {{/each}}
        </table>
        <hr>
    </div>
    {{else}}
    <h3 class="alert alert-warning">No se encontraron productos</h3>
    {{/if}}
</div>