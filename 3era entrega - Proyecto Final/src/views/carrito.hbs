    <div class="jumbotron">
    <h1>Vista de productos en carrito</h1>
    <br>
    {{#if hayProductos}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>
            {{#each productos}}
            <tr>
                <td>{{this.nombre}}</td>
                <td>S/ {{this.precio}}</td>
                <td><img width="50" src={{this.url}} alt="not found"></td>
            </tr>
            {{/each}}
        </table>
        <hr>
        <button class="btn btn-danger" onclick="location.href='/carritos/{{carrito}}/pedido'">PEDIR</button>
    </div>
    {{else}}
    <h3 class="alert alert-warning">No se encontraron productos</h3>
    {{/if}}
</div>